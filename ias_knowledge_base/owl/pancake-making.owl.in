<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY pancake "http://ias.cs.tum.edu/kb/pancake-making.owl#" >
    <!ENTITY knowrob "http://ias.cs.tum.edu/kb/knowrob.owl#" >
    <!ENTITY computable "http://ias.cs.tum.edu/kb/computable.owl#" >
]>


<rdf:RDF xmlns="http://ias.cs.tum.edu/kb/pancake-making.owl#"
     xml:base="http://ias.cs.tum.edu/kb/pancake-making.owl"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:knowrob="http://ias.cs.tum.edu/kb/knowrob.owl#">
    <owl:Ontology rdf:about="http://ias.cs.tum.edu/kb/pancake-making.owl">
        <owl:imports rdf:resource="file://@OWL_PATH_PREFIX@/owl/knowrob.owl"/>
    </owl:Ontology>



    <!--  ######  OBJECTS  ###### -->

    <owl:NamedIndividual rdf:about="&pancake;egg1">
        <rdf:type rdf:resource="&knowrob;Egg-Chickens"/>
        <knowrob:heightOfObject rdf:datatype="&xsd;float">0.05</knowrob:heightOfObject>
        <knowrob:depthOfObject rdf:datatype="&xsd;float">0.05</knowrob:depthOfObject>
        <knowrob:widthOfObject rdf:datatype="&xsd;float">0.05</knowrob:widthOfObject>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&pancake;pancakemix1">
        <rdf:type rdf:resource="&knowrob;PancakeMix"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&pancake;milk1">
        <rdf:type rdf:resource="&knowrob;CowsMilk-Product"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&pancake;pancakemaker1">
        <rdf:type rdf:resource="&knowrob;PancakeMaker"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&pancake;plate1">
        <rdf:type rdf:resource="&knowrob;DinnerPlate"/>
    </owl:NamedIndividual>


    <owl:NamedIndividual rdf:about="&pancake;table1">
        <rdf:type rdf:resource="&knowrob;KitchenTable"/>
        <knowrob:heightOfObject rdf:datatype="&xsd;float">0.75</knowrob:heightOfObject>
        <knowrob:depthOfObject rdf:datatype="&xsd;float">1.0</knowrob:depthOfObject>
        <knowrob:widthOfObject rdf:datatype="&xsd;float">1.0</knowrob:widthOfObject>
    </owl:NamedIndividual>



    <!--  ######  ACTIONS  ###### -->

    <!-- heat up the pancake maker -->
    <owl:NamedIndividual rdf:about="&pancake;turnon1">
        <rdf:type rdf:resource="&knowrob;TurningOnPoweredDevice"/>
        <knowrob:objectActedOn rdf:resource="&pancake;pancakemaker1"/>
        <knowrob:startTime rdf:resource="&pancake;timepoint_1298634548"/>
    </owl:NamedIndividual>


    <!-- crack the egg, create eggshell and eggyolk -->
    <owl:NamedIndividual rdf:about="&pancake;cracking1">
        <rdf:type rdf:resource="&knowrob;Cracking"/>
        <knowrob:objectActedOn rdf:resource="&pancake;egg1"/>
        <knowrob:startTime rdf:resource="&pancake;timepoint_1298634568"/>
    </owl:NamedIndividual>


    <!-- mix eggyolk with milk and pancake mix to dough -->
    <owl:NamedIndividual rdf:about="&pancake;mixing1">
        <rdf:type rdf:resource="&knowrob;Mixing"/>
        <knowrob:objectActedOn rdf:resource="&knowrob;EggYolk-Food1"/> <!-- how to refer to instances that will only be created later on? -->
        <knowrob:objectActedOn rdf:resource="&pancake;milk1"/>
        <knowrob:objectActedOn rdf:resource="&pancake;pancakemix1"/>
        <knowrob:startTime rdf:resource="&pancake;timepoint_1298634588"/>
    </owl:NamedIndividual>


    <!-- pour dough onto pancake maker -->
    <owl:NamedIndividual rdf:about="&pancake;pour1">
        <rdf:type rdf:resource="&knowrob;PuttingSomethingOnto"/>
        <knowrob:objectActedOn rdf:resource="&knowrob;Dough2"/>
        <knowrob:toLocation rdf:resource="&pancake;pancakemaker1"/>
        <knowrob:startTime rdf:resource="&pancake;timepoint_1298634608"/>
    </owl:NamedIndividual>


    <!-- flip the pancake ?? -->


    <!-- put pancake onto plate -->
    <owl:NamedIndividual rdf:about="&pancake;put1">
        <rdf:type rdf:resource="&knowrob;PuttingSomethingOnto"/>
        <knowrob:objectActedOn rdf:resource="&knowrob;Baked4"/>
        <knowrob:toLocation rdf:resource="&pancake;plate1"/>
        <knowrob:startTime rdf:resource="&pancake;timepoint_1298634628"/>
    </owl:NamedIndividual>


    <!-- table pose -->

    <owl:NamedIndividual rdf:about="&pancake;SemanticMapPerception1">
        <rdf:type rdf:resource="&knowrob;VisualPerception"/>
        <knowrob:eventOccursAt rdf:resource="&pancake;RotationMatrix3D1"/>
        <knowrob:startTime rdf:resource="&pancake;timepoint_1286380475"/>
        <knowrob:objectActedOn rdf:resource="&pancake;table1"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&pancake;RotationMatrix3D1">
        <rdf:type rdf:resource="&knowrob;RotationMatrix3D"/>
        <knowrob:m00 rdf:datatype="&xsd;float">1.0</knowrob:m00>
        <knowrob:m01 rdf:datatype="&xsd;float">0.0</knowrob:m01>
        <knowrob:m02 rdf:datatype="&xsd;float">0.0</knowrob:m02>
        <knowrob:m03 rdf:datatype="&xsd;float">0.0</knowrob:m03>

        <knowrob:m10 rdf:datatype="&xsd;float">0.0</knowrob:m10>
        <knowrob:m11 rdf:datatype="&xsd;float">1.0</knowrob:m11>
        <knowrob:m12 rdf:datatype="&xsd;float">0.0</knowrob:m12>
        <knowrob:m13 rdf:datatype="&xsd;float">0.0</knowrob:m13>

        <knowrob:m20 rdf:datatype="&xsd;float">0.0</knowrob:m20>
        <knowrob:m21 rdf:datatype="&xsd;float">0.0</knowrob:m21>
        <knowrob:m22 rdf:datatype="&xsd;float">1.0</knowrob:m22>
        <knowrob:m23 rdf:datatype="&xsd;float">0.375</knowrob:m23>

        <knowrob:m30 rdf:datatype="&xsd;float">0.0</knowrob:m30>
        <knowrob:m31 rdf:datatype="&xsd;float">0.0</knowrob:m31>
        <knowrob:m32 rdf:datatype="&xsd;float">0.0</knowrob:m32>
        <knowrob:m33 rdf:datatype="&xsd;float">1.0</knowrob:m33>
    </owl:NamedIndividual>


    <!-- egg pose -->

    <owl:NamedIndividual rdf:about="&pancake;SemanticMapPerception2">
        <rdf:type rdf:resource="&knowrob;VisualPerception"/>
        <knowrob:eventOccursAt rdf:resource="&pancake;RotationMatrix3D2"/>
        <knowrob:startTime rdf:resource="&pancake;timepoint_1286380475"/>
        <knowrob:objectActedOn rdf:resource="&pancake;egg1"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&pancake;RotationMatrix3D2">
        <rdf:type rdf:resource="&knowrob;RotationMatrix3D"/>
        <knowrob:m00 rdf:datatype="&xsd;float">1.0</knowrob:m00>
        <knowrob:m01 rdf:datatype="&xsd;float">0.0</knowrob:m01>
        <knowrob:m02 rdf:datatype="&xsd;float">0.0</knowrob:m02>
        <knowrob:m03 rdf:datatype="&xsd;float">0.0</knowrob:m03>

        <knowrob:m10 rdf:datatype="&xsd;float">0.0</knowrob:m10>
        <knowrob:m11 rdf:datatype="&xsd;float">1.0</knowrob:m11>
        <knowrob:m12 rdf:datatype="&xsd;float">0.0</knowrob:m12>
        <knowrob:m13 rdf:datatype="&xsd;float">0.0</knowrob:m13>

        <knowrob:m20 rdf:datatype="&xsd;float">0.0</knowrob:m20>
        <knowrob:m21 rdf:datatype="&xsd;float">0.0</knowrob:m21>
        <knowrob:m22 rdf:datatype="&xsd;float">1.0</knowrob:m22>
        <knowrob:m23 rdf:datatype="&xsd;float">0.3775</knowrob:m23>

        <knowrob:m30 rdf:datatype="&xsd;float">0.0</knowrob:m30>
        <knowrob:m31 rdf:datatype="&xsd;float">0.0</knowrob:m31>
        <knowrob:m32 rdf:datatype="&xsd;float">0.0</knowrob:m32>
        <knowrob:m33 rdf:datatype="&xsd;float">1.0</knowrob:m33>
    </owl:NamedIndividual>


    <owl:NamedIndividual rdf:about="&pancake;timepoint_1286380475">
        <rdf:type rdf:resource="&knowrob;TimePoint"/>
    </owl:NamedIndividual>


    <owl:NamedIndividual rdf:about="&pancake;timepoint_1298634548">
        <rdf:type rdf:resource="&knowrob;TimePoint"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&pancake;timepoint_1298634568">
        <rdf:type rdf:resource="&knowrob;TimePoint"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&pancake;timepoint_1298634588">
        <rdf:type rdf:resource="&knowrob;TimePoint"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&pancake;timepoint_1298634608">
        <rdf:type rdf:resource="&knowrob;TimePoint"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&pancake;timepoint_1298634628">
        <rdf:type rdf:resource="&knowrob;TimePoint"/>
    </owl:NamedIndividual>

</rdf:RDF>


