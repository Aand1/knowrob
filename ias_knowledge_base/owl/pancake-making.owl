<rdf:RDF xmlns="http://ias.cs.tum.edu/kb/pancake-making.owl#"
     xml:base="http://ias.cs.tum.edu/kb/pancake-making.owl"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:knowrob="http://ias.cs.tum.edu/kb/knowrob.owl#">
    <owl:Ontology rdf:about="http://ias.cs.tum.edu/kb/pancake-making.owl">
        <owl:imports rdf:resource="file:///work/overlay/stacks/knowrob/ias_knowledge_base/owl/knowrob.owl"/>
    </owl:Ontology>



    <!--  ######  OBJECTS  ###### -->

    <owl:NamedIndividual rdf:about="&pancake;egg1">
        <rdf:type rdf:resource="&knowrob;Egg-Chickens"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&pancake;pancakemix1">
        <rdf:type rdf:resource="&knowrob;PancakeMix"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&pancake;milk1">
        <rdf:type rdf:resource="&knowrob;CowsMilk-Product"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&pancake;pancakemaker1">
        <rdf:type rdf:resource="&knowrob;PancakeMaker"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&pancake;plate1">
        <rdf:type rdf:resource="&knowrob;DinnerPlate"/>
    </owl:NamedIndividual>



    <!--  ######  ACTIONS  ###### -->

    <!-- heat up the pancake maker -->
    <owl:NamedIndividual rdf:about="&pancake;turnon1">
        <rdf:type rdf:resource="&knowrob;TurningOnPoweredDevice"/>
        <knowrob:objectActedOn rdf:resource="&pancake;pancakemaker1"/>
    </owl:NamedIndividual>


    <!-- crack the egg, create eggshell and eggyolk -->
    <owl:NamedIndividual rdf:about="&pancake;cracking1">
        <rdf:type rdf:resource="&knowrob;Cracking"/>
        <knowrob:objectActedOn rdf:resource="&pancake;egg1"/>
    </owl:NamedIndividual>


    <!-- mix eggyolk with milk and pancake mix to dough -->
    <owl:NamedIndividual rdf:about="&pancake;mixing1">
        <rdf:type rdf:resource="&knowrob;Mixing"/>
        <knowrob:objectActedOn rdf:resource="&knowrob;EggYolk-Food1"/> <!-- how to refer to instances that will only be created later on? -->
        <knowrob:objectActedOn rdf:resource="&pancake;milk1"/>
        <knowrob:objectActedOn rdf:resource="&pancake;pancakemix1"/>
    </owl:NamedIndividual>


    <!-- pour dough onto pancake maker -->
    <owl:NamedIndividual rdf:about="&pancake;pour1">
        <rdf:type rdf:resource="&knowrob;PuttingSomethingOnto"/>
        <knowrob:objectActedOn rdf:resource="&knowrob;Dough2"/>
        <knowrob:toLocation rdf:resource="&pancake;pancakemaker1"/>
    </owl:NamedIndividual>


    <!-- flip the pancake ?? -->


    <!-- put pancake onto plate -->
    <owl:NamedIndividual rdf:about="&pancake;put1">
        <rdf:type rdf:resource="&knowrob;PuttingSomethingOnto"/>
        <knowrob:objectActedOn rdf:resource="&knowrob;Baked3"/>
        <knowrob:toLocation rdf:resource="&pancake;plate1"/>
    </owl:NamedIndividual>



</rdf:RDF>



