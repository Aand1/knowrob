	    </owl:Restriction>
	    <owl:Restriction>
                <owl:onProperty rdf:resource="&comp_cap;launchFile"/>
                <owl:someValuesFrom rdf:resource="&comp_cap_roslaunch;collision_map_self_occ.launch"/>
	    </owl:Restriction>
        </owl:intersectionOf>
    </owl:Class>

