    <!-- http://ias.cs.tum.edu/kb/srdl2-cap.owl#BaseCollisionCheckCapability -->

    <owl:Class rdf:about="&srdl2-cap;BaseCollisionCheckCapability">
	<owl:intersectionOf rdf:parseType="Collection">
             <owl:Restriction>
                <owl:onProperty rdf:resource="&comp_cap;dependsOnSubscribedMessageType"/>
                <owl:someValuesFrom rdf:resource="&comp_cap_topics;sensor_msgs_._LaserScan"/>
            </owl:Restriction>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&comp_cap;dependsOnSubscribedMessageType"/>
                <owl:someValuesFrom rdf:resource="&comp_cap_topics;sensor_msgs_._PointCloud"/>
            </owl:Restriction>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&comp_cap;dependsOnPublishedMessageType"/>
                <owl:someValuesFrom rdf:resource="&comp_cap_topics;nav_msgs_._Odometry"/>
            </owl:Restriction>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&comp_cap;dependsOnPublishedMessageType"/>
                <owl:someValuesFrom rdf:resource="&comp_cap_topics;tf_._tfMessage"/>
            </owl:Restriction>
	</owl:intersectionOf>
    </owl:Class>
     
