    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
      
    <!-- http://ias.cs.tum.edu/kb/comp_cap.owl#dependsOnMessageType -->
     
    <owl:ObjectProperty rdf:about="&comp_cap;dependsOnMessageType">
      <rdfs:domain rdf:resource="&srdl2-cap;Capability"/>
      <rdfs:range  rdf:resource="&comp_cap_topics;Topic"/>
       <rdfs:subPropertyOf rdf:resource="&srdl2;dependsOn"/>
    </owl:ObjectProperty>
    
    <!-- http://ias.cs.tum.edu/kb/comp_cap.owl#dependsOnSubscribedMessageType -->
    
    <owl:ObjectProperty rdf:about="&comp_cap;dependsOnSubscribedMessageType">
      <rdfs:domain rdf:resource="&srdl2-cap;Capability"/>
      <rdfs:range  rdf:resource="&comp_cap_topics;Topic"/>
      <rdfs:subPropertyOf rdf:resource="&comp_cap;dependsOnMessageType"/>
    </owl:ObjectProperty>
    
    <!-- http://ias.cs.tum.edu/kb/comp_cap.owl#dependsOnPublishedMessageType -->
    
    <owl:ObjectProperty rdf:about="&comp_cap;dependsOnPublishedMessageType">
      <rdfs:domain rdf:resource="&srdl2-cap;Capability"/>
      <rdfs:range  rdf:resource="&comp_cap_topics;Topic"/>
      <rdfs:subPropertyOf rdf:resource="&comp_cap;dependsOnMessageType"/>
    </owl:ObjectProperty>
    
    <!-- http://ias.cs.tum.edu/kb/comp_cap.owl#dependsOnService -->
    
    <owl:ObjectProperty rdf:about="&comp_cap;dependsOnService">
      <rdfs:domain rdf:resource="&srdl2-cap;Capability"/>
      <rdfs:range  rdf:resource="&comp_cap_topics;Service"/>
      <rdfs:subPropertyOf rdf:resource="&comp_cap;dependsOn"/>
    </owl:ObjectProperty>
    
    <!-- http://ias.cs.tum.edu/kb/comp_cap.owl#package -->
     
    <owl:ObjectProperty rdf:about="&comp_cap;rospackage">
      <rdfs:domain rdf:resource="&srdl2-cap;Capability"/>
      <rdfs:range  rdf:resource="&comp_cap_roslaunch;Rospackage"/>
    </owl:ObjectProperty>
    
    <!-- http://ias.cs.tum.edu/kb/comp_cap.owl#launchFile -->
     
    <owl:ObjectProperty rdf:about="&comp_cap;launchFile">
      <rdfs:domain rdf:resource="&srdl2-cap;Capability"/>
      <rdfs:range  rdf:resource="&comp_cap_roslaunch;Roslaunch"/>
    </owl:ObjectProperty>
           
    <!-- 
